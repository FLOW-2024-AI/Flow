# n8n Webhook Configuration
N8N_WEBHOOK_URL=your_n8n_webhook_url

# Airtable Configuration
AIRTABLE_API_KEY=your_airtable_api_key
AIRTABLE_BASE_ID=your_airtable_base_id
AIRTABLE_TABLE_NAME=Facturas

# PostgreSQL RDS Configuration
DB_HOST=your_rds_endpoint
DB_NAME=facturas_db
DB_USER=flow_app_rw
DB_PASSWORD=your_db_password
DB_PORT=5432

# Facturas proxy configuration
FACTURAS_API_URL=https://api.flowcfo.com
NEXT_PUBLIC_FACTURAS_API=https://api.flowcfo.com
NEXT_PUBLIC_FACTURAS_MODE=real

# AWS S3 Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# Cognito Auth (multi-tenant)
COGNITO_REGION=us-east-1
COGNITO_USER_POOL_ID=your_user_pool_id
COGNITO_CLIENT_ID=your_app_client_id
COGNITO_CLIENT_SECRET=your_app_client_secret

# Tenant settings
ALLOW_DEV_TENANT=false
DEV_TENANT_ID=cliente
BASE_PREFIX=facturas
ALLOW_S3_TENANT_BYPASS=false
TENANT_EMAIL_DOMAIN=flow-cfo.com
DEFAULT_TENANT_ID=unknown
